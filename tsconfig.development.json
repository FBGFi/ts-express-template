{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "./dev",
        "baseUrl": ".",
        "noEmit": false,
        "paths": {
            "*": [
                "./node_modules/*"
            ]
        }
    },
    "include": [
        "./**/*",
        "./index.ts"
    ]
}